{
  "expo": {
    "name": "Calories AI",
    "slug": "calorie-from-photo-ai",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.caloriefromphotoai"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/foreground.png",
        "backgroundColor": "#6847c0"
      },
      "package": "com.anonymous.caloriefromphotoai"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/icons/icon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/icons/foreground.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#6847c0"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
